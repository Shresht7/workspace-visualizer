{
  "name": "@workspace-visualizer/file-color-associations",
  "version": "1.0.0",
  "description": "File color associations for workspace visualizer",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "node build/index.js",
    "build": "npm run build:tsc && npm run build:export",
    "build:tsc": "tsc",
    "build:export": "node ./build/scripts/exportFileColorAssociations.js ./fileColors.json",
    "watch": "tsc -w",
    "test": "vitest",
    "clean": "npx rimraf build node_modules"
  },
  "keywords": [
    "workspace-visualizer",
    "file-colors",
    "file-color-associations"
  ],
  "author": "Shresht7",
  "repository": {
    "type": "git",
    "url": "https://github.com/Shresht7/workspace-visualizer/"
  },
  "license": "MIT"
}
