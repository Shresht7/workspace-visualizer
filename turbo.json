{
    "$schema": "https://turbo.build/schema.json",
    "pipeline": {
        "build": {
            "outputs": [
                "build/**",
                "dist/**"
            ],
            "dependsOn": ["^build"]
        },
        "watch": {
            "outputs": [
                "build/**",
                "dist/**"
            ]
        },
        "test": {
            "outputs": [
                "build/**",
                "dist/**"
            ],
            "dependsOn": ["^build"]
        }
    }
}
