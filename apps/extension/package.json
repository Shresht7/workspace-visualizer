{
  "name": "@workspace-visualizer/vscode-workspace-visualizer",
  "displayName": "vscode-workspace-visualizer",
  "description": "Visualize your workspace",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:workspace-visualizer.workspace-side-view",
    "onCommand:vscode-workspace-visualizer.helloWorld"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "views": {
      "explorer": [
        {
          "type": "webview",
          "id": "workspace-visualizer.workspace-side-view",
          "name": "Graph View"
        }
      ]
    },
    "commands": [
      {
        "command": "vscode-workspace-visualizer.show-force-directed-graph",
        "title": "Show Force Directed Graph"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "build": "npm run compile",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "#test": "node ./dist/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vscode/test-electron": "^2.2.0",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0"
  },
  "dependencies": {
    "ignore": "^5.2.4"
  }
}
